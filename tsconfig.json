{
  "extends": "./node_modules/expo/tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "importHelpers": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}